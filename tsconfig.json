{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "skipLibCheck": true,
    "paths": {
    },
    /* Bundler mode */
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    /* Linting */
    "strict": true,
    "allowJs": true,
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "types": ["vite/client"],
    /* Output */
    "noEmit": false,
    "outDir": "dist",
    "rootDir": "src",
    "declaration": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["./src"]
}
